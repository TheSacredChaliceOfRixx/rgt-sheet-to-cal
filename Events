    <script>
      function courseFormatter(value, row) {
          return `<a href="${row.roadDetailsUrl}">${value}</a>`
      }

      function eventDetailsFormatter(value, row) {
         return `<a class="btn btn-round btn-outline-primary" href="${value}">Details</a>`
      }

      function dateFormatter(value, row) {
          return `${row.elevationGained} ${row.elevationLost}`
      }

      function finishedFormatter(value, row) {
          console.log(value)
          return value ? '' : '<i class="fas fa-dot-circle text-success"></i>'
      }

      function getQueryParams(params) {
          return params
      }

     function loadingTemplate() {
          return '<i class="fa fa-spinner fa-spin fa-fw fa-2x"></i>'
      }
    </script>


<script>
    document.getElementById('timeZoneSelect').addEventListener('change',
        function(ev) {
            var [offset, name] = ev.target.value.split(',')
            Cookies.set('timezone', offset, { SameSite: 'Lax', expires: 3650 })
            Cookies.set('timezoneName', name, { SameSite: 'Lax', expires: 3650 })
            location.reload()
        })
</script>
